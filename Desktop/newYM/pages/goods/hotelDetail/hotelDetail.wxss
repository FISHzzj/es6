/* pages/goods/hotelDetail/hotelDetail.wxss */
page {
  background: #f5f5f5;
  height: 100%;
}

.top {
  position: relative;
  height: 356rpx;
  width: 100%;
  font-size: 28rpx;
  border-bottom: 1px solid #e5e5e5;
  box-sizing: border-box;
}

.top>cover-view {
  width: 100%;
  height: 110rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  color: #333;
  background: rgba(255, 255, 255, 0.705);
  box-sizing: border-box;
  padding: 20rpx 20rpx 20rpx 40rpx;
  z-index: 1
}

.shopName {
  display: inline-block;
  color: #333;
  width: 460rpx;
  height: 100%;
}

.top>cover-view cover-image {
  float: right;
  width: 44rpx;
  height: 40rpx;
}

.shopImg {
  width: 100%;
  height: 100%;
}

.shopImg image{
  width: 100%;
  height: 100%;
}

.locBox{
  background: white;
  padding: 20rpx 0;
}

.locBox>image{
  width: 32rpx;
  height: 32rpx;
  padding: 0 28rpx;
}

.locInfo{
  width: 540rpx;
  min-height: 60rpx;
  font-size: 24rpx;
  color: #333;
  padding-left: 20rpx;
}

.phone{
  width: 36rpx;
  height: 38rpx;
  padding: 20rpx 28rpx !important;
  border-left: 2px solid #f2f2f2;
}

.dateSelect{
  position: relative;
  margin: 20rpx;
  border: 1px solid #e5e5e5;
  background: white;
  padding: 6rpx 0;
  box-sizing: border-box;
}

.dateSelect>view{
  width: 50%;
  text-align: center;
}

.dateSelect>span{
  position: absolute;
  display: inline-block;
  color: #333;
  border: 1px solid #e5e5e5;
  font-size: 28rpx;
  height: 52rpx;
  width: 110rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 6rpx;
  left: calc(50% - 55rpx);
  top: calc(50% - 26rpx);
}

.status{
  color: #999;
  font-size: 24rpx;
  line-height: 50rpx;
}

.status+view{
  font-size: 32rpx;
  color: #ec7043;
  font-weight: bold;
}

.datePiker{
  width: 100%;
  height: 500rpx;
}

.typeSelect{
  background: white;
  padding: 10rpx 24rpx 30rpx;
  color: #333;
  font-size: 24rpx;
}

.typeSelect>label{
  width: 24%;
  height: 56rpx;
  border: 1px solid #f0f0f0;
  text-align: center;
  line-height: 56rpx;
  box-sizing: border-box;
  margin: 4rpx;
}

.typeSelect checkbox{
  display: none;
}

.act{
  background: #fcf0eb;
  color: #ed7b52
}

.room{
  background: white;
  height: 210rpx;
  width: 100%;
  border-top: 1px solid #fbfbfb;
}

.roomImg{
  width: 140rpx;
  height: 144rpx;
  margin: 32rpx 24rpx;
}

.roomImg+view{
  width: 320rpx;
  margin-top: 20rpx;
}

.roomImg+view view{
  font-size: 24rpx;
  color: #9e9e9e;
  line-height: 46rpx;
}

.roomTitle{
  color: #333 !important;
  font-size: 28rpx !important;
  margin-bottom: 4rpx;
}

.price{
  font-size: 26rpx;
  color: #ec7043;
}

.reservation{
  width: 120rpx;
  height: 60rpx;
  background: #ec7043;
  color: white;
  font-size: 28rpx;
  text-align: center;
  border-radius: 6rpx;
  line-height: 60rpx;
  margin: 72rpx 0 0 10rpx;
}

.commentTitle{
  height: 80rpx;
  margin-top: 30rpx;
  color: #333;
  font-size: 24rpx;
  background: white;
  padding: 0 32rpx 0 24rpx;
}

.commentTitle>span{
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-width: 1px 1px 0 0;
  border-color: #939393;
  border-style: solid;
  transform: rotate(45deg);
}

.commentList{
  padding-bottom: 40rpx;
}

.comment{
  width: 100%;
  background: white;
  padding: 20rpx;
  border-top: 1px solid #eaeaea;
}

.commentTop>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}

.userInfo{
  flex: 1;
  color: #333;
  font-size: 24rpx;
  padding: 10rpx 34rpx 0 20rpx;
  box-sizing: border-box;
}

.userInfo>view{
  width: 64%;
}


.userInfo>text{
  color: #d1d1d1;
  padding-left: 30rpx;
}

.star>image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 6rpx;
}

.commentContent{
  padding: 10rpx 26rpx 0 110rpx;
}

.textBox{
  color: #898989;
  font-size: 28rpx;

}

.imgBox{
  margin-top: 20rpx;

}

.imgBox>image{
  width: 170rpx;
  height: 170rpx;
  margin: 0 10rpx 10rpx 0;
}

.mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .6);
  z-index: 11;
}

.roomDetail{
  position: fixed;
  top: 70rpx;
  left: 3%;
  width: 94%;
  height: 1000rpx;
  background: white;
  border-radius: 10rpx;
  border: 1px solid #bbb;
  z-index: 11;
  overflow: hidden;
}

.detailImg{
  width: 100%;
  height: 340rpx;

}

.detailImg image{
  width: 100%;
  height: 100%;
}

.detailImg+cover-image{
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  width: 46rpx;
  height: 46rpx;
  background: rgba(255, 255, 255, 0.6)
}

.roomName{
  position: absolute;
  top: 16rpx;
  left: 30rpx;
  font-size: 32rpx;
  color: white;

}

.roomInfo{
  padding: 20rpx 24rpx 40rpx;
  border-bottom: 1px solid #f6f6f6;
}

.condition{
  width: 50%;
  color: #333;
  font-size: 24rpx;
  line-height: 36rpx;

}

.ruleBox{
  height: 350rpx;
  padding: 20rpx 24rpx;
  box-sizing: border-box;
}

.condition>text{
  color: #8a8a8a;
  padding-right: 24rpx;
}

.ruleTitle{
  color: #333;
  font-size: 28rpx;
  padding-bottom: 6rpx;
}

.li{
  color: #8a8a8a;
  font-size: 24rpx;
  text-indent: 40rpx;
}

.reservationBtn{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  background: #ec7043;
  color: white;
  line-height: 90rpx;
  text-align: center;
  font-size: 28rpx;
}