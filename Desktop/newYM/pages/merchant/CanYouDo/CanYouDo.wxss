/* pages/CanYouDo/CanYouDo.wxss */
page{
  background: #f5f5f5;
  font-size: 28rpx;

}

.h3 {
    padding: 10rpx 0;
    text-align: center;
}
.content {
    background-color: white; 
}
.content .tr {
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    height: 80rpx;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.tr .th {
    margin-right: 20rpx;
}
.tr .td {
    color: rgba(0, 0, 0, 0.5);
    font-size: 28rpx;
}

.imgLine{
  height: 120rpx;
  padding: 0 24rpx;
  border-bottom: 1px solid #e9e9e9;
}

.imgLine>image{
  width: 90rpx;
  height: 90rpx;
  margin-left: 24rpx;
}