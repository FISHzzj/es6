<!--pages/merchant/billingDetails/journal.wxml-->
<view class='billingDetails'>
  <!-- 表头 -->
  <view class='header'>
    <text>日期</text>
    <text>订单号</text>
    <text>消费类型</text>
    <text>消费金额</text>
  </view>
  <!-- 主内容 -->
  <view class='main'>
    <view class='content' wx:for="{{list}}" wx:key="index">
      <text>{{item.createtime}}</text>
      <text>{{item.ordersn}}</text>
      <text>{{item.type}}</text>
      <text>{{item.price}}</text>
    </view>
  </view>
</view>