<!--pages/merchant/checkOrderList/checkOrderList.wxml-->
<view class='title flex'>
  <view>核销时间</view>
  <view style='flex:1'>订单号</view>
  <view style='width:20%;'>消费金额</view>
</view>
<view style='height:80rpx;'></view>
<view class='line flex' wx:for="{{list}}" wx:key="index">
  <view>{{item.checktime}}</view>
  <view style='flex:1'>{{item.ordersn}}</view>
  <view style='width:20%;'>{{item.itemprice}}</view>
</view>