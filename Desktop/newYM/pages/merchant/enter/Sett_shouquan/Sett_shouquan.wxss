page{
  background: #f5f5f5;
}
.head_box{
  width: 100%;
  height: 60rpx;
  background: #e5e5e5;
  text-align: center;
  font-size: 26rpx;
  line-height: 60rpx;
  color: #000;
}

.text_div{
  width: 100%;
  height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  border-bottom: 2rpx solid #e5e5e5;
}
.butt_box{
  width: 100%;
 
  margin-top: 40rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
 
  text-align: center;
}
.baktix{
  font-size: 24rpx;
  color: #a4a4a4;
  padding: 0 40rpx;
  box-sizing: border-box;
  line-height: 24rpx;
  height: 40rpx;
  margin-top: 30rpx;
}
.butt_box button{
  width: 100%;
  background: #e532a0;
  color: #fff;
  border-radius: 10rpx;
   height: 74rpx;
  line-height: 74rpx;
  font-size: 26rpx;
}

