/* pages/Sett_zhenjian/Sett_zhenjian.wxss */
page{
  background: #f5f5f5;
}
.head{
  width: 100%;
  height: 60rpx;
  background: #e5e5e5;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
}
.text_div{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0  26rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  border-bottom: 2rpx solid #e5e5e5;
}
.tisi{
  margin-left: 20rpx;
  color: #818181;
}
.img_div{
  width: 100%;
  height: 132rpx;
  padding:  0  26rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #e5e5e5;
}
.img_div image{
  width: 76rpx;
  height: 76rpx;
  border-radius: 10rpx;
  background: white;
  margin-right: 30rpx;
}
.butt_box{
  width: 100%;
  height: 74rpx;
  line-height: 74rpx;
  margin-top: 40rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
.butt_box view{
  width: 100%;
  background: #e532a0;
  color: #fff;
  border-radius: 10rpx;
}

.imgBox{
  position: relative;
  height: 76rpx;
  width: 76rpx;
  margin-right: 30rpx;
}

.imgBox>span{
  position: absolute;
  right: 0;
  top: 0;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: red;
  font-weight: bold;
  font-size: 30rpx;
  background: rgba(255, 255, 255, .6)
}