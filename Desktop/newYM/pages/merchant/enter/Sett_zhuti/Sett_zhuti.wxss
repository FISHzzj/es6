/* pages/Sett_zhuti/Sett_zhuti.wxss */
.head_box{
  width: 100%;
  height: 60rpx;
  background: #e5e5e5;
  text-align: center;
  font-size: 26rpx;
  line-height: 60rpx;
  color: #000;
}
.content_box view{
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  font-size: 26rpx;
  border-bottom: 2rpx solid #e5e5e5;
  padding:  0 28rpx;
  box-sizing: border-box;
  
}
.xuanzhe{
  display: flex;
}
.flex-al-cen{
  display: flex;
  align-items: center;
}
.content_box view input{
  margin-left: 20rpx;
}
.boxs{
  box-sizing: border-box;
}
.right{
  margin-right: 30rpx;
  
}
.tlae{
  width: 75%;
}
.butt_box{
  width: 100%;
  height: 74rpx;
  line-height: 74rpx;
  margin-top: 40rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  
  text-align: center;
}
.butt_box button{
  width: 100%;
  background: #e532a0;
  height: 74rpx;
  line-height: 74rpx;
  color: #fff;
  border-radius: 10rpx;
  font-size: 26rpx;
}
.right::before{
    content: " ";
    display: inline-block;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
    height: 10px;
    width: 10px;
    border-width: 2px 2px 0 0;
    border-color: #8e8e8e;
    border-style: solid;
}