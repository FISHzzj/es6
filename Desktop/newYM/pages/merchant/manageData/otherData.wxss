/* pages/merchant/manageData/otherData.wxss */
page{
  background: #f5f5f5;
  font-size: 28rpx;
}

.top .li {
    padding: 0 20rpx; 
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 7vh;
    border-bottom: 1px solid  rgba(0, 0, 0, 0.1);
}
.setOpacity {
    color: #979797;
    padding-left: 6rpx;
}
/* 表格 */
.tableHead .th, .tableBody .tr {
    display: flex;
    align-items: center;
    text-align: center;
    font-size: 28rpx;
    height: 2rem;
    box-sizing: border-box;
    padding: 0 5rpx;
}
.tableHead .th {
    height: 2.3rem;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.tableBody .tr text{
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    font-size: 28rpx;
}
.tableBody .tr text:not(:last-child) {
    border-right: 1px solid rgba(0, 0, 0, 0.1);
}
.th text, .tr text {
    width: 25vw;
}
.getMore {
  height: 80rpx;
  line-height: 60rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
    text-align: center;
}