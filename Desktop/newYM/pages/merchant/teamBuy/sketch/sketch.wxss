/* pages/attribute/attribute.wxss */
.text_box{
  width: 100%;
  height: 60rpx;
  background: #e5e5e5;
  text-align: center;
  color: #000;
  font-size: 28rpx;
  line-height: 60rpx;
  margin-bottom: 10rpx;
}
.contempt_box view{
  width: 100%;
  display: flex;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  padding: 0 24rpx;
  box-sizing: border-box;
  color: #535353;
  /* border-bottom: 2rpx solid #eee; */
}
.contempt_box view input{
  height: 100%;
  color: #000;
}
.bili{
  display: flex;
  align-items: center;
  padding-right: 0 !important;
  margin-bottom: 10rpx;
}
.bili_inpuy{
  width: 70%;
  height: 60rpx !important;
  border: 2rpx solid #eee;
  padding-left: 10rpx;
}
/* 下一步 */
.buttan{
  width: 90%;
  height: 74rpx;
  background: #e3329f;
  margin: 20rpx auto;
  text-align: center;
  font-size: 26rpx;
  line-height: 74rpx;
  border-radius: 10rpx;
  color: #fff;
}

.img{
  padding: 0 24rpx;
  color: #333;
  font-size: 24rpx;
}

.imgList{
  width: 520rpx;
  height: 110rpx;
  overflow: hidden;
}

.add{
  background: whitesmoke;
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx 0 0 20rpx;
}

.imgBox{
  position: relative;
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  margin: 20rpx 0 0 20rpx;
}

.imgBox>image{
  width: 80rpx;
  height: 80rpx;
}

.imgBox>span{
  display: inline-block;
  position: absolute;
  right: 0;
  top: 0;
  width: 26rpx;
  height: 26rpx;
  text-align: center;
  line-height: 26rpx;
  color: red;
  font-size: 32rpx;
  font-weight: bold;
  background: rgba(255, 255, 255, 0.4);
}