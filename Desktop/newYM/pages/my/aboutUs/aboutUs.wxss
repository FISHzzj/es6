/* pages/aboutUs/aboutUs.wxss */
.head {
    height: 35vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.head image {
    width: 15vw;
    height: 15vw;
    border-radius: 50%;
    margin-bottom: 40rpx;
}
.head view {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.head view text {
    font-size: 0.75rem;
    margin-bottom: 10rpx;
}
.head view text:last-child {
    font-size: 0.7rem;
}
.info {
    font-size: 0.8rem;
    padding: 0 20rpx;
    line-height: 1.5em;
}
.copyRight {
    position: absolute;
    bottom: 20rpx;
    font-size: 0.6rem;
    width: 100%;
    text-align: center;
}