/* pages/my/team/team.wxss */
page{
  background: #f5f5f5;
}

.top{
  border-bottom: 1px solid #e4e4e4;
  margin-bottom: 20rpx;
}

.tabBtn{
  height: 160rpx;
  width: 50%;
  background: white;
  padding-top: 44rpx;
  text-align: center;
  color: #7e7e7e;
  font-size: 32rpx;
  line-height: 48rpx;
  box-sizing: border-box;
}

.act{
  color: #333;
  font-weight: bold;
  border-bottom: 2px solid #e532a0;
}

.memberItem{
  height: 140rpx;
  background: white;
  border-bottom: 1px solid #ebebeb;
  color: #868686;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.headImg{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}

.headImg+image{
  width: 36rpx;
  height: 36rpx;
  margin: 0 16rpx;

}

.memberItem text{
  color: #333;
  font-weight: bold;
  padding-right: 6rpx;
}